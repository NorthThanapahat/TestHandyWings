<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>TestHandyWings</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ParkingCalculate</servlet-name>
    <servlet-class>com.thanaphat.Test.CalculateParkingPriceServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SplitAddressServlet</servlet-name>
    <servlet-class>com.thanaphat.Test.SplitAddressServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CalculateCompensationServlet</servlet-name>
    <servlet-class>com.thanaphat.Test.CalculateCompensationServlet</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>SaveEmployee</servlet-name>
    <servlet-class>com.thanaphat.Test.SaveEmployee</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ParkingCalculate</servlet-name>
    <url-pattern>/calculate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SplitAddressServlet</servlet-name>
    <url-pattern>/splitAddress</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SaveEmployee</servlet-name>
    <url-pattern>/saveEmployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CalculateCompensationServlet</servlet-name>
    <url-pattern>/calculateCompensation.jsp</url-pattern>
  </servlet-mapping>
</web-app>